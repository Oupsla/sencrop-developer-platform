{
  "name": "sencrop-developer-platform",
  "version": "1.9.0",
  "description": "The Sencrop Developer Platform website",
  "main": "index.js",
  "metapak": {
    "configs": [
      "main",
      "readme",
      "oss"
    ],
    "data": {
      "files": "components/**/*.js pages/*.js next.config.js",
      "ignore": [
        "out",
        ".next"
      ]
    }
  },
  "scripts": {
    "build": "next build",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "checkStaged": "if ! git diff-files --quiet --ignore-submodules ; then echo \"⚠️ - Unstaged files found:\"; echo $(git diff-files --shortstat); fi;",
    "cli": "env NODE_ENV=${NODE_ENV:-cli}",
    "cpswaggerstyles": "cp node_modules/swagger-ui-dist/swagger-ui.css static/styles/swagger-ui.css",
    "dev": "next",
    "export": "next export",
    "lint": "echo \"WARNING: No linter configured\"",
    "metapak": "metapak",
    "postinstall": "npm run metapak --silent",
    "prebuild": "npm run cpswaggerstyles",
    "precz": "npm run lint && npm run metapak -- -s",
    "preversion": "npm run lint && npm run metapak -- -s",
    "start": "next start",
    "version": "npm run changelog"
  },
  "keywords": [],
  "author": "",
  "contributors": [
    "Cynthia HENAFF <cynthia+npm@henaff.io> (https://henaff.io)"
  ],
  "license": "MIT",
  "dependencies": {
    "next": ">=8.0.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-ga": "^2.5.7",
    "react-no-ssr": "^1.1.0",
    "react-syntax-highlighter": "^10.2.1",
    "swagger-ui-dist": "^3.22.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "babel-plugin-inline-import": "^3.0.0",
    "conventional-changelog-cli": "^2.0.11",
    "cz-conventional-changelog": "^2.1.0",
    "dotenv": "^7.0.0",
    "husky": "^1.3.1",
    "metapak": "^3.1.6",
    "metapak-sencrop": "7.3.0"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "config": {},
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run checkStaged"
    }
  }
}
