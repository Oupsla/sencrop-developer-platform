{
  "name": "sencrop-developer-platform",
  "version": "1.0.3",
  "description": "The Sencrop Developer Platform website",
  "main": "index.js",
  "metapak": {
    "configs": [
      "main",
      "readme",
      "oss"
    ],
    "data": {
      "files": "components/**/*.js pages/*.js next.config.js",
      "ignore": [
        "out",
        ".next"
      ]
    }
  },
  "scripts": {
    "build": "next build",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "cli": "env NODE_ENV=${NODE_ENV:-cli}",
    "cpswaggerdefinition": "cp node_modules/sencrop-js-api-client/src/swagger.api.json static/swagger.api.json",
    "cpswaggerstyles": "cp node_modules/swagger-ui-dist/swagger-ui.css static/styles/swagger-ui.css",
    "cz": "env NODE_ENV=${NODE_ENV:-cli} git cz",
    "dev": "next",
    "export": "next export",
    "lint": "echo \"WARNING: No linter configured\"",
    "metapak": "metapak || exit 0",
    "postinstall": "npm run metapak --silent",
    "prebuild": "npm run cpswaggerstyles && npm run cpswaggerdefinition",
    "preversion": "npm run lint",
    "start": "next start",
    "version": "npm run changelog && git add CHANGELOG.md"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "babel-plugin-styled-components": "^1.2.1",
    "babel-plugin-transform-assets-import-to-string": "^1.0.1",
    "next": "^4.0.3",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-ga": "^2.3.5",
    "react-no-ssr": "^1.1.0",
    "react-syntax-highlighter": "^5.7.0",
    "sencrop-js-api-client": "^1.1.1",
    "styled-components": "^2.2.1",
    "swagger-ui-dist": "^3.3.1"
  },
  "devDependencies": {
    "babel-plugin-inline-import": "^2.0.6",
    "commitizen": "^2.9.6",
    "conventional-changelog-cli": "^1.3.4",
    "css-loader": "^0.28.7",
    "cz-conventional-changelog": "^2.0.0",
    "metapak": "0.0.21",
    "metapak-sencrop": "^3.0.1",
    "style-loader": "^0.19.0"
  },
  "engines": {
    "node": ">=6.10.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
